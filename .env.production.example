# Production ENV settings
# Скопіюйте це в .env на production сервері

APP_ENV=production
APP_DEBUG=false

# Redis password (ВАЖЛИВО: встановіть сильний пароль!)
REDIS_PASSWORD=your-strong-redis-password-here

# Database
DB_CONNECTION=pgsql
DB_HOST=db
DB_PORT=5432
DB_DATABASE=postgres
DB_USERNAME=laravel
DB_PASSWORD=your-strong-db-password-here

# MINIO
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=your-strong-minio-password-here

